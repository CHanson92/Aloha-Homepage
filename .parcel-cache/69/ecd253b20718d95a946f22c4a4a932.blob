    grid-column: 1/3;
    grid-row: 1;
  }
  .categories .trends {
    grid-column: 3;
    grid-row: 1;
  }
  .categories .glasses {
    grid-column: 4;
    grid-row: 1;
  }
  .categories .sale {
    grid-column: 1;
    grid-row: 2;
  }
  .categories .bags {
    grid-column: 2;
    grid-row: 2;
  }
  .categories .womens {
    grid-column: 3/5;
    grid-row: 2;
  }

  /* Updates grid */
  .updates {
    display: grid;
    grid-template-rows: 1fr 1fr 1fr;
    grid-template-columns: 1fr 1fr 1fr;
  }
  .updates h1 {
    grid-row: 1;
    grid-column: 1/4;
  }
  .updates p {
    grid-row: 2;
    grid-column: 1/4;
  }
  .updates form {
    grid-row: 3;
    grid-column: 2/3;
  }

  /* Footer grid */
  footer {
    display: grid;
    grid-template-columns: 1fr 1fr 1fr;
  }
  footer .details {
    grid-column: 1;
  }
  footer .logo {
    grid-column: 2;
  }
  footer .socialmedia {
    grid-column: 3;
    grid-row: 1;
  }
}
