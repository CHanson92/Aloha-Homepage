@media screen and (min-width: 0px) {
  p {
    font-size: 14px;
  }

  /* Header requirements */
  header .Aloha-logo {
    display: flex;
    justify-content: left;
    border-bottom: 2px solid #d7d7d7;
  }
  header .Aloha-logo img {
    height: auto;
    max-width: 10rem;
  }
  header nav {
    display: flex;
    justify-self: center;
    align-self: center;
    font-size: 1rem;
    margin: 1em;
  }
  header nav ul li {
    display: inline-block;
    padding-right: 1rem;
  }
  header nav a:link,
header nav a:visited,
header nav a:active {
    color: #e2574c;
    text-decoration: none;
  }
  header .Cart-logo {
    display: flex;
    justify-content: flex-end;
    border-bottom: 2px solid #d7d7d7;
  }
  header .Cart-logo img {
    height: auto;
    max-width: 3.875rem;
  }

  /* Class Background */
  .class-background {
    height: 33em;
    background-size: contain, cover;
  }

  /* Categories */
  .categories {
    height: 50em;
  }
  .categories .mens {
    background-size: cover;
    background-repeat: no-repeat;
  }
  .categories .trends {
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }
  .categories .glasses {
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }
  .categories .sale {
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }
  .categories .bags {
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
  }
  .categories .womens {
    background-size: cover;
    background-repeat: no-repeat;
  }

  /* Most loved products */
  .mostlovedproducts .carousel-cell {
    width: 100%;
  }

  /* Footer */
  footer {
    padding-bottom: 2.5rem;
    text-align: center;
  }
  footer .details {
    justify-self: center;
    line-height: 1.25rem;
    margin-top: 2.5rem;
  }
  footer .details .address {
    margin-top: 1.25rem;
  }
  footer .logo {
    display: none;
  }
  footer .socialmedia ul {
    display: flex;
    flex-direction: row;
    justify-content: center;
  }
  footer .socialmedia ul li {
    padding-top: 1.5rem;
  }
  footer .socialmedia ul li a i {
    min-width: 25px;
  }
  footer .socialmedia ul li a span {
    display: none;
  }
}
