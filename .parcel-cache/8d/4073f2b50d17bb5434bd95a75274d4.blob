  }
  .categories .sale {
    grid-column: 1;
    grid-row: 2;
  }
  .categories .bags {
    grid-column: 2;
    grid-row: 2;
  }
  .categories .womens {
    grid-column: 3/5;
    grid-row: 2;
  }

  /* Updates grid */
  .updates {
    display: grid;
    grid-template-rows: 1fr 1fr 1fr;
    grid-template-columns: 1fr 1fr 1fr;
  }
  .updates h1 {
    grid-row: 1;
